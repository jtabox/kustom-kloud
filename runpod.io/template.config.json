// honestly, this isn't any official format, just putting the values in a handmade json so i don't forget them
{
    "Template Name": "AI-Dock - Comfy UI",
    "Template Type": "Pod",
    "Compute Type": "Nvidia GPU",
    "Container Image": "ghcr.io/ai-dock/comfyui:v2-cuda-12.1.1-base-22.04-v0.2.0",
    "Container Registry Credentials": "",
    "Container Start Command": "",
    "Container Disk GB": 6,
    "Volume Disk GB": 60,
    "Volume Mount Path": "/workspace",
    "Expose HTTP Ports": [1111,8188,8384,8888,41648],
    "Expose TCP Ports": [22,2222,28188,28888,22999,41650],
    "Template Visibility": "Private",
    "Environment Variables": {
        "CF_QUICK_TUNNELS": false,
        "PROVISIONING_SCRIPT": "https://raw.githubusercontent.com/jtabox/kustom-kloud/main/runpod.io/ai-dock.comfyui.provisioning.sh",
        "WORKSPACE": "/workspace",
        "HETZ_DRIVE": "ofvn",
        "HETZ_USER": "{{ RUNPOD_SECRET_HETZ_USER }}",
        "HETZ_PWD_RO": "{{ RUNPOD_SECRET_HETZ_PWD_RO }}",
        "HF_TOKEN": "{{ RUNPOD_SECRET_HF_TOKEN }}",
        "CIVITAI_API_KEY": "{{ RUNPOD_SECRET_CIVITAI_API_KEY }}",
        "OPENAI_API_KEY": "{{ RUNPOD_SECRET_OPENAI_API_KEY }}"
    }
}